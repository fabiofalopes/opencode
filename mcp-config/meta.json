{
  "meta_aggregator_magg": {
    "type": "local",
    "command": [
      "docker",
      "run",
      "-i",
      "--rm",
      "-p",
      "8000:8000",
      "ghcr.io/sitbon/magg"
    ],
    "enabled": true,
    "timeout": 30000
  },
  "context_manager_llm": {
    "type": "local",
    "command": [
      "uvx",
      "llm-context"
    ],
    "enabled": true,
    "timeout": 30000
  }
}
