{
  "ollama_bridge": {
    "type": "local",
    "command": [
      "npx",
      "-y",
      "ollama-mcp-server"
    ],
    "enabled": false,
    "timeout": 10000
  },
  "qdrant_mcp": {
    "type": "local",
    "command": [
      "docker",
      "run",
      "-i",
      "--rm",
      "-e",
      "QDRANT_URL",
      "-e",
      "OPENAI_API_KEY",
      "delorenj/mcp-qdrant-memory"
    ],
    "environment": {
      "QDRANT_URL": "http://host.docker.internal:6333",
      "OPENAI_API_KEY": "needs-configuration-or-local-alternative"
    },
    "enabled": false,
    "timeout": 10000
  },
  "neo4j_mcp": {
    "type": "local",
    "command": [
      "docker",
      "run",
      "-i",
      "--rm",
      "-e",
      "NEO4J_URI",
      "-e",
      "NEO4J_USERNAME",
      "-e",
      "NEO4J_PASSWORD",
      "mcp/neo4j-memory"
    ],
    "environment": {
      "NEO4J_URI": "bolt://host.docker.internal:7687",
      "NEO4J_USERNAME": "neo4j",
      "NEO4J_PASSWORD": "password"
    },
    "enabled": false,
    "timeout": 10000
  }
}
