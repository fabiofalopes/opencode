{
  "integration_suite_aio": {
    "type": "local",
    "command": [
      "docker",
      "run",
      "-i",
      "--rm",
      "--env-file",
      ".env",
      "ghcr.io/athapong/aio-mcp"
    ],
    "enabled": true,
    "timeout": 30000
  },
  "integration_notes_hackmd": {
    "type": "local",
    "command": [
      "npx",
      "-y",
      "hackmd-mcp"
    ],
    "environment": {
      "HACKMD_API_TOKEN": "{env:HACKMD_API_TOKEN}"
    },
    "enabled": true,
    "timeout": 30000
  },
  "integration_github_repos": {
    "type": "local",
    "command": [
      "npx",
      "-y",
      "github-repos-manager-mcp"
    ],
    "environment": {
      "GITHUB_TOKEN": "{env:GITHUB_PERSONAL_ACCESS_TOKEN}"
    },
    "enabled": true,
    "timeout": 30000
  }
}
