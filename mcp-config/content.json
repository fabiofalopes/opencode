{
  "content_pdf_marker": {
    "type": "local",
    "command": [
      "docker",
      "run",
      "-i",
      "--rm",
      "xiaoyao9184/marker"
    ],
    "enabled": true,
    "timeout": 60000
  },
  "content_pdf_reader": {
    "type": "local",
    "command": [
      "docker",
      "run",
      "-i",
      "--rm",
      "-v",
      "${PWD}:/pdfs",
      "mcp/pdf-reader"
    ],
    "enabled": true,
    "timeout": 30000
  }
}
