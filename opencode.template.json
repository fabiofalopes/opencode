{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "opencode-google-antigravity-auth"
  ],
  "instructions": [
    "AGENTS.md",
    "docs/SYSTEM_CONTEXT.md",
    "docs/MCP_TOOLS.md"
  ],
  "model": "{profile:model}",
  "mcp": {
    "ollama_bridge": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "ollama-mcp-server"
      ],
      "enabled": false,
      "timeout": 10000
    },
    "qdrant_mcp": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "QDRANT_URL",
        "-e",
        "OPENAI_API_KEY",
        "delorenj/mcp-qdrant-memory"
      ],
      "environment": {
        "QDRANT_URL": "http://host.docker.internal:6333",
        "OPENAI_API_KEY": "needs-configuration-or-local-alternative"
      },
      "enabled": false,
      "timeout": 10000
    },
    "neo4j_mcp": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "NEO4J_URI",
        "-e",
        "NEO4J_USERNAME",
        "-e",
        "NEO4J_PASSWORD",
        "mcp/neo4j-memory"
      ],
      "environment": {
        "NEO4J_URI": "bolt://host.docker.internal:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "password"
      },
      "enabled": false,
      "timeout": 10000
    },
    "code_interpreter": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-v",
        "{machine:mcp-code}:/workspace",
        "mcp/mcp-code-interpreter"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "github": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "environment": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "{env:GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "enabled": false,
      "timeout": 10000
    },
    "content_pdf_marker": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "xiaoyao9184/marker"
      ],
      "enabled": true,
      "timeout": 120000
    },
    "content_pdf_reader": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-pdf"
      ],
      "enabled": true,
      "timeout": 30000
    },
    "context7": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@dolasoft/stdio-context7-mcp-server"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "gh_grep": {
      "type": "remote",
      "url": "https://mcp.grep.app",
      "enabled": true,
      "timeout": 10000
    },
    "duckduckgo": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/duckduckgo"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "fetch": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/fetch"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "brave": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "BRAVE_API_KEY",
        "mcp/brave-search"
      ],
      "environment": {
        "BRAVE_API_KEY": "dummy-key-not-used"
      },
      "enabled": false,
      "timeout": 10000
    },
    "perplexity": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "PERPLEXITY_API_KEY",
        "mcp/perplexity-ask"
      ],
      "environment": {
        "PERPLEXITY_API_KEY": "{env:PERPLEXITY_API_KEY}"
      },
      "enabled": true,
      "timeout": 10000
    },
    "obsidian": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "OBSIDIAN_HOST",
        "-e",
        "OBSIDIAN_API_KEY",
        "mcp/obsidian"
      ],
      "environment": {
        "OBSIDIAN_HOST": "host.docker.internal",
        "OBSIDIAN_API_KEY": "{env:OBSIDIAN_API_KEY}"
      },
      "enabled": true,
      "timeout": 10000
    },
    "integration_suite_aio": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "--env-file",
        ".env",
        "ghcr.io/athapong/aio-mcp"
      ],
      "enabled": false,
      "timeout": 30000
    },
    "integration_notes_hackmd": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "hackmd-mcp"
      ],
      "environment": {
        "HACKMD_API_TOKEN": "{env:HACKMD_API_TOKEN}"
      },
      "enabled": false,
      "timeout": 30000
    },
    "integration_github_repos": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "github-repos-manager-mcp"
      ],
      "environment": {
        "GITHUB_TOKEN": "{env:GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "enabled": false,
      "timeout": 30000
    },
    "sequential_thinking": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/sequentialthinking"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "memory": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-v",
        "{machine:mcp-memory}:/data",
        "mcp/memory"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "memory_vector_qdrant": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "QDRANT_URL",
        "-e",
        "QDRANT_API_KEY",
        "mcp/qdrant"
      ],
      "environment": {
        "QDRANT_URL": "{env:QDRANT_URL}",
        "QDRANT_API_KEY": "{env:QDRANT_API_KEY}"
      },
      "enabled": true,
      "timeout": 10000
    },
    "meta_aggregator_magg": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-p",
        "8090:8000",
        "ghcr.io/sitbon/magg"
      ],
      "enabled": false,
      "timeout": 30000
    },
    "context_manager_llm": {
      "type": "local",
      "command": [
        "uvx",
        "llm-context"
      ],
      "enabled": false,
      "timeout": 30000
    },
    "wikipedia": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/wikipedia-mcp"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "arxiv": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "ARXIV_STORAGE_PATH",
        "-v",
        "{machine:arxiv-papers}:/papers",
        "mcp/arxiv-mcp-server"
      ],
      "environment": {
        "ARXIV_STORAGE_PATH": "/papers"
      },
      "enabled": true,
      "timeout": 10000
    },
    "paper_search": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/paper-search"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "hackernews": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/hackernews-mcp"
      ],
      "enabled": true,
      "timeout": 10000
    },
    "research_scraper_firecrawl": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "FIRECRAWL_API_KEY",
        "mcp/firecrawl"
      ],
      "environment": {
        "FIRECRAWL_API_KEY": "{env:FIRECRAWL_API_KEY}"
      },
      "enabled": true,
      "timeout": 60000
    },
    "research_search_searxng": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "--network",
        "host",
        "-e",
        "SEARXNG_ENGINE_API_BASE_URL",
        "overtlids/mcp-searxng-enhanced"
      ],
      "environment": {
        "SEARXNG_ENGINE_API_BASE_URL": "{env:SEARXNG_BASE_URL}"
      },
      "enabled": true,
      "timeout": 30000
    },
    "research_browser_rag": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@apify/mcp-server-rag-web-browser"
      ],
      "environment": {
        "APIFY_TOKEN": "{env:APIFY_TOKEN}"
      },
      "enabled": false,
      "timeout": 30000
    }
